<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xmlns="http://www.springframework.org/schema/beans" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:property-placeholder/>

    <dubbo:reference id="tripAbilityTransCommonDubboRpcService" check="true"  version="${dubbo.consumer.trip.version}" group="${dubbo.consumer.trip.group}"
                     timeout="${dubbo.consumer.trip.timeout}" loadbalance="${dubbo.consumer.trip.loadbalance}"
                     interface="com.fusion.center.trip.interfaces.TripAbilityTransCommonDubboRpcService"/>

</beans>